Program Pzim ;
Var
ResA, ResB, ResC, ResD, ResE: Real;	
Begin
    Writeln('12 x 4 = ?');
    ResA:=12+12+12+12;
    Writeln('Resultado:', ResA:5:2);
    Writeln('');
    Writeln('23 x 11 = ? ');
    ResB:=23+23+23+23+23+23+23+23+23+23+23;
    Writeln('Resultado:', ResB:5:2);
    Writeln('');
    Writeln('10 ÷ 2: ?');
    ResC:=10-5;
    Writeln('Resultado:', ResC:5:2);
    Writeln('');
    Writeln('175 ÷ 7 = ? ');
    ResD:=175-25-25-25-25-25-25;
    Writeln('Resultado:', ResD:5:2);
    Writeln('');
    Writeln('28 x 5 = ? ');
    ResE:=28+28+28+28+28;
    Writeln('Resultado:', ResE:5:2);
End.